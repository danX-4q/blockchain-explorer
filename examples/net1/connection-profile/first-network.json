{
	"name": "first-network",
	"version": "1.0.0",
	"client": {
		"tlsEnable": true,
		"adminUser": "admin",
		"adminPassword": "adminpw",
		"enableAuthentication": false,
		"organization": "Org11MSP",
		"connection": {
			"timeout": {
				"peer": {
					"endorser": "300"
				},
				"orderer": "300"
			}
		}
	},
	"channels": {
		"zrchannel": {
			"peers": {
				"peer0.org11.example.com": {}
			},
			"connection": {
				"timeout": {
					"peer": {
						"endorser": "6000",
						"eventHub": "6000",
						"eventReg": "6000"
					}
				}
			}
		}
	},
	"organizations": {
		"Org11MSP": {
			"mspid": "Org11MSP",
			"fullpath": false,
			"adminPrivateKey": {
				"path": "/tmp/crypto/peerOrganizations/org11.example.com/users/Admin@org11.example.com/msp/keystore"
			},
			"signedCert": {
				"path": "/tmp/crypto/peerOrganizations/org11.example.com/users/Admin@org11.example.com/msp/signcerts"
			}
		}
	},
	"peers": {
		"peer0.org11.example.com": {
			"tlsCACerts": {
				"path": "/tmp/crypto/peerOrganizations/org11.example.com/peers/peer0.org11.example.com/tls/ca.crt"
			},
			"url": "grpcs://peer0.org11.example.com:7051",
			"eventUrl": "grpcs://peer0.org11.example.com:7053",
			"grpcOptions": {
				"ssl-target-name-override": "peer0.org11.example.com"
			}
		}
	}
}
